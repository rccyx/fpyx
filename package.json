{
  "name": "fpyx",
  "version": "1.0.0",
  "description": "Hardened cross-runtime request fingerprinting for anonymous rate limiting. Zero dependencies. Zero I/O. Zero crypto APIs. Works in Node.js 18+, Bun, Deno, Cloudflare Workers, edge runtimes, and modern browsers. Fast FNV-1a 64-bit hashing with trusted IP detection.",
  "keywords": [
    "rate-limiting",
    "request-fingerprint",
    "fingerprint",
    "anonymous",
    "fnv",
    "fnv1a",
    "hash",
    "hashing",
    "edge-runtime",
    "cloudflare-workers",
    "vercel-edge",
    "netlify-edge",
    "fastly",
    "fly-io",
    "bun",
    "deno",
    "nodejs",
    "typescript",
    "zero-dependencies",
    "cross-runtime",
    "api-security",
    "ddos-protection",
    "quota",
    "throttle",
    "ip-detection"
  ],
  "homepage": "https://github.com/rccyx/fpyx#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/rccyx/fpyx.git"
  },
  "bugs": {
    "url": "https://github.com/rccyx/fpyx/issues"
  },
  "author": {
    "name": "@rccyx"
  },
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "browser": "./dist/index.mjs",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "build": "rimraf dist && rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install",
    "release": "semantic-release",
    "test": "vitest --run --coverage"
  },
  "devDependencies": {
    "@cspell/dict-cryptocurrencies": "5.0.0",
    "@rollup/plugin-replace": "5.0.5",
    "@rollup/plugin-typescript": "11.1.5",
    "@semantic-release/changelog": "6.0.3",
    "@types/eslint": "8.56.10",
    "@types/node": "20.10.7",
    "@types/rollup-plugin-auto-external": "2.0.5",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitest/coverage-istanbul": "1.5.0",
    "@vitest/coverage-v8": "1.2.2",
    "can-npm-publish": "1.3.6",
    "cspell": "8.3.2",
    "dotenv": "16.4.6",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-functional": "6.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsdoc": "48.0.2",
    "eslint-plugin-markdown": "3.0.1",
    "eslint-plugin-n": "16.6.1",
    "eslint-plugin-optimize-regex": "1.2.1",
    "eslint-plugin-prettier": "5.1.2",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-unicorn": "51.0.1",
    "eslint-plugin-vitest": "0.3.20",
    "husky": "9.0.7",
    "knip": "4.2.0",
    "prettier": "3.1.1",
    "prettier-plugin-packagejson": "2.4.9",
    "rimraf": "5.0.5",
    "rollup": "4.9.4",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-dts": "6.1.0",
    "semantic-release": "23.0.2",
    "testyx": "1.4.0",
    "ts-node": "^10.9.2",
    "ts-roids": "^1.45.0",
    "tsx": "4.19.2",
    "typedoc": "0.25.13",
    "typescript": "5.3.3",
    "vite-tsconfig-paths": "4.2.3",
    "vitest": "1.5.0",
    "zod": ">=3.0.0"
  }
}
